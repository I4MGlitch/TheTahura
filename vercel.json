{
    "version": 2,
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/tahuraproject/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/tahuraproject/$1"
      }
    ]
  }
  